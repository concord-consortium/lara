- embeddable_string = "#{embeddable.class.to_s.demodulize.underscore}_#{embeddable.id}"
.multiple_choice
  .prompt= embeddable.prompt.html_safe
  = form_for embeddable, :remote => true
    - embeddable.choices.each do |choice|
      = f.label embeddable, choice, choice.choice
        = f.radio_button embeddable, choice
  %button{ :onclick => "checkAnswer('embeddable__#{embeddable_string}');" } Check Answer
