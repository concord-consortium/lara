.question-hdr
  %h5.h5= "Question ##{embeddable.question_index}"
.question-bd
  .question-txt
    != embeddable.prompt
  = form_for embeddable,
    :remote => true,
    :html => {:'data-type' => 'json'} do |f|
    = f.text_area :answer_text, :class => 'live_submit', :rows => "8", :placeholder => "Type answer here"
