{
  "name": "lara-typescript",
  "version": "0.0.1",
  "description": "LARA TypeScript code",
  "scripts": {
    "publish:plugin-api": "npm run build && cd dist/plugin-api && npm publish --access public",
    "publish:plugin-api:beta": "npm run build && cd dist/plugin-api && npm publish --tag beta --access public",
    "publish:interactive-api-client": "npm run build && cd dist/interactive-api-client && npm publish --access public",
    "publish:interactive-api-client:beta": "npm run build && cd dist/interactive-api-client && npm publish --tag beta --access public",
    "publish:interactive-api-client:yalc": "npm run build && cd dist/interactive-api-client && npx yalc publish",
    "publish:interactive-api-host": "npm run build && cd dist/interactive-api-host && npm publish --access public",
    "publish:interactive-api-host:beta": "npm run build && cd dist/interactive-api-host && npm publish --tag beta --access public",
    "publish:interactive-api-host:yalc": "npm run build && cd dist/interactive-api-host && npx yalc publish",
    "build": "npm-run-all lint:build clean build:webpack build:types build:docs",
    "build:dev": "npm-run-all lint:build build:webpack",
    "build:webpack": "webpack --devtool false && npm run build:copy-to-rails",
    "build:webpack:no-copy-to-rails": "webpack --devtool false",
    "build:types": "rollup -c",
    "build:docs": "npm-run-all build:docs:plugin-api build:docs:interactive-api-client build:docs:interactive-api-host",
    "build:docs:plugin-api": "typedoc --readme src/plugin-api/README.md --out ../docs/lara-plugin-api src/plugin-api/index.ts",
    "build:docs:interactive-api-client": "typedoc --readme src/interactive-api-client/README.md --out ../docs/interactive-api-client src/interactive-api-client/index.ts",
    "build:docs:interactive-api-host": "typedoc --readme src/interactive-api-host/README.md --out ../docs/interactive-api-host src/interactive-api-host/index.ts",
    "build:dev:lara-typescript": "webpack --config webpack-laraTypescript.config.js && npm run build:copy-to-rails",
    "build:watch": "webpack --watch",
    "build:copy-to-rails": "node scripts/copy-to-rails.js",
    "clean": "rimraf dist docs ../docs/lara-plugin-api ../docs/interactive-api-client",
    "lara-api:link": "npm run build:webpack && cd dist/interactive-api-client && npm link && cd ../..",
    "lara-api:unlink": "cd dist/interactive-api-client && npm unlink && cd ../..",
    "list:links": "npm ls -g --depth=0 --link=true",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:build": "tslint -c tslint-build.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint-build.json --fix 'src/**/*.{ts,tsx}'",
    "lint:unused": "tsc --noUnusedLocals --project .",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watch",
    "test:coverage:watch": "jest --coverage --watchAll",
    "test:full": "npm-run-all test test:cypress",
    "example-interactives": "npx live-server --port=8888 ./dist/example-interactives/",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/lara.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/lara/issues"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  },
  "homepage": "https://github.com/concord-consortium/lara/tree/master/lara-typescript#readme",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.14.9",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/deep-freeze": "^0.1.2",
    "@types/dompurify": "^2.2.3",
    "@types/jest": "^26.0.24",
    "@types/jquery": "^3.5.6",
    "@types/jqueryui": "^1.12.16",
    "@types/react": "^16.14.11",
    "@types/react-beautiful-dnd": "^13.1.1",
    "@types/react-dom": "^16.9.14",
    "@types/react-select": "^3.1.2",
    "@types/react-tabs": "^2.3.3",
    "@types/semver": "^7.3.9",
    "@types/showdown": "^2.0.6",
    "@types/uuid": "^8.3.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^6.4.1",
    "cpx": "^1.5.0",
    "css-loader": "^5.2.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-fetch-mock": "^1.7.5",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^1.6.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.2",
    "rollup-plugin-dts": "^3.0.2",
    "sass": "^1.62.1",
    "sass-loader": "^10.2.0",
    "storybook-react-context": "^0.4.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.2.0",
    "tslint-react-hooks": "^2.2.2",
    "typedoc": "0.17.0-3",
    "typedoc-plugin-markdown": "^2.4.2",
    "typescript": "^4.4.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.30.0",
    "@aws-sdk/s3-request-presigner": "^3.30.0",
    "@concord-consortium/interactive-api-host": "^0.7.1",
    "@concord-consortium/slate-editor": "^0.7.3",
    "@concord-consortium/text-decorator": "^1.0.2",
    "@concord-consortium/token-service": "^2.1.0",
    "@tinymce/tinymce-react": "^3.12.7",
    "@types/react-query": "^1.2.9",
    "classnames": "^2.3.1",
    "deep-freeze": "0.0.1",
    "dompurify": "^2.3.0",
    "eventemitter2": "^6.4.4",
    "html-react-parser": "^1.2.7",
    "iframe-phone": "^1.3.1",
    "immer": "^9.0.6",
    "markdown-to-jsx": "^6.11.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-mde": "^10.0.3",
    "react-query": "^3.21.0",
    "react-select": "^3.2.0",
    "react-tabs": "^3.2.2",
    "react-tooltip": "^4.2.6",
    "semver": "^7.3.5",
    "showdown": "^2.1.0",
    "ts-debounce": "^4.0.0",
    "use-immer": "^0.6.0",
    "uuid": "^8.3.2"
  }
}
